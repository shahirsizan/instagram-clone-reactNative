{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(tabs)/index.jsx",
		"app/_layout.jsx",
		"convex/tasks.js",
		"styles/create.styles.js",
		"convex/posts.js",
		"convex/http.js"
	]
}
